<?xml version="1.0" ?>

<!--
This URDF contains a single link with, in order:
1) A convex-declared mesh, which should parse into geometry::Convex.
2) A mesh that is explicitly declared to not be convex, which should
  parse into geometry::Mesh.
3) A mesh that is not declared to be either, which should parse into
  geometry::Mesh.
-->
<robot name="non_conflicting_materials_2">
  <link name="base_link">
   <collision>
      <geometry>
        <mesh convex="True" filename="../tri_cube.obj" scale="1.1 1.1 1.1"/>
      </geometry>
    </collision>

   <collision>
      <geometry>
        <mesh convex="False" filename="../tri_cube.obj" scale="1.1 1.1 1.1"/>
      </geometry>
    </collision>

   <collision>
      <geometry>
        <mesh filename="../tri_cube.obj" scale="1.1 1.1 1.1"/>
      </geometry>
    </collision>
  </link>
</robot>
